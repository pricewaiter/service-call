{
    "extends": "airbnb-base",
    "rules": {
        "indent": [2, 4],
        "global-require": [0],
        "no-console": [2],
        "comma-dangle": ["error", {
            "objects": "always-multiline",
            "imports": "always-multiline",
            "exports": "always-multiline",
            "functions": "ignore"
        }]
    }
}
